{
  "name": "ynab-helper",
  "version": "0.1.0",
  "main": "resources/main.js",
  "scripts": {
    "build-electron": "lein trampoline cljsbuild auto electron-dev",
    "watch-electron": "sleep 10 && nodemon -w resources/main.js --exec electron .",
    "electron": "run-p -l build-electron watch-electron",
    "less": "less-watch-compiler resources/public/less resources/public/css main.less",
    "figwheel": "rlwrap lein trampoline figwheel frontend-dev",
    "lein-repl": "lein repl",
    "sourcemap-server": "http-server -p 9988 resources/public",
    "start": "run-p -l electron less lein-repl sourcemap-server"
  },
  "devDependencies": {
    "electron": "^1.6.6",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2"
  },
  "dependencies": {
    "antd": "2.8.0",
    "glob-fs": "^0.1.6",
    "less": "^2.7.2",
    "less-watch-compiler": "^1.4.2"
  }
}
